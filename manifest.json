{
    "manifest_version": 3,
    "name": "ToDo",
    "version": "0.0.3",
    "description": "A todo addon",
    "permissions": ["identity"],
    "host_permissions": [],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "images/icon-16.png",
            "48": "images/icon-48.png",
            "128": "images/icon-128.png"
        }
    },
    "icons": {
        "16": "images/icon-16.png",
        "48": "images/icon-48.png",
        "128": "images/icon-128.png"
    },
    "browser_specific_settings": {
        "gecko": {
            "id": "{6cddf72c-86ff-40fc-852f-eab2850db7f5}",
            "strict_min_version": "109.0"
        }
    },
    "author": "Ethan Schafstall"
}
